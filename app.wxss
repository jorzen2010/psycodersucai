/**app.wxss**/
page{
  background-color: #efefef;
}
.whitebg{
  background-color: #ffffff;
}
.head-title{
  font-size: 14pt;
  color: #353535;
}
.head-title-small{
  font-size: 10pt;
  line-height: 20pt;
  color: #888888;
}
.content-title{
  font-size: 12pt;
  line-height: 25pt;
  color: #353535;
  font-weight: 600;
}
.content-title-small{
  font-size: 10pt;
  line-height: 14pt;
  color: #888888;
}




.searchbox{
  border: 1px solid #efefef;
  height: 30px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #efefef;
  padding: 3px;
}
.search-img{
  height: 25px;
  width: 25px;
  border-radius: 5px 0px 0px 5px;
  background-color: #ffffff;
}
.searchinput{
  background-color: #ffffff;
  border-radius: 0px 5px 5px 0px;
  height: 25px;
  width: 100%;
}
.searchplaceholder{
  font-size: 12px;
  text-align: center;
}
