page{
  background-color: #ffffff;
}
.scollcontainer{
  margin-top: 0px;
  white-space: nowrap;
  width: 100%;
  display: flex;
  padding-top: 5px;
  padding-bottom: 5px;
  position: fixed;
  z-index: 999;
}
.scrollitem{
  width: 80px;
  height: 30px;
  margin: 3px;
  display: inline-block;
}

.scrollitem-img{
  width: 80px;
  height: 30px;
  border: 1px solid #cccccc;
  border-radius: 5px;
}
.toppaixu{
  padding:50px 5px 5px 5px;
  z-index: 998;
  position: fixed;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  border-bottom: 1px solid #efefef;
}
.toppaixu text{
  color: #bfbfbf;
  font-size: 10pt;
  padding-left: 5px;

}
.picker{
  margin-right: 20px;
  color: #bfbfbf;
  font-size: 10pt;
}
.picker image{
  width: 12px;
  height: 12px;
}


.listContainer{
